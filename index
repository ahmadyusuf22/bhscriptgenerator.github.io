   <script type="text/javascript">
            var vm = new Vue({
                el: "#app",
                data: {
                  
                    interface1: "",
                    interface2: "",
                   
                    iphotspot: "",
                    ipppoe: "",
                    ipisp2: "",
                    isp2 : false,
                    pppoe : false,
                    t_down: "",
                    t_up: "",
                    limit_game_down: "",
                    limit_game_up: "",
                    hasil_down: 0,
                    hasil_up: 0,
                },
                methods: {
                    kirim() {
                        this.interface1 =
                            document.getElementById("interface1").value;
                        this.interface2 =
                            document.getElementById("interface2").value;
                          
                        this.iphotspot =
                            document.getElementById("iphotspot").value;
                        this.ipppoe = document.getElementById("ipppoe").value;
                        this.ipisp2 = document.getElementById("ipisp2").value;
                        this.t_up = document.getElementById("t_up").value;
                        this.t_down = document.getElementById("t_down").value;
                        this.limit_game_down = document.getElementById("limit_game_down").value;
                        this.limit_game_up = document.getElementById("limit_game_up").value;

                        if(this.ipisp2!=''){
                            this.isp2=true;
                        }
                        if(this.ipppoe!=''){
                            this.pppoe=true;
                        }

                        this.hasil_down = parseInt(this.t_down) - parseInt(this.limit_game_down);
                        this.hasil_up = parseInt(this.t_up) - parseInt(this.limit_game_up);

                    },
                },
            });
        </script>
  
